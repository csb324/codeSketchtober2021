{
  "name": "codesketchtober",
  "version": "1.0.0",
  "description": "creative coding challenges for the month of october",
  "main": "app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "server": "nodemon app.js",
    "build": "webpack --watch --mode=development",
    "start": "concurrently \"npm run server\" \"npm run build\"",
