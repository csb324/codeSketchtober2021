    "express-handlebars": "^5.3.3",
    "handlebars": "^4.7.7",
    "handlebars-layouts": "^3.1.4",
    "p5": "^1.4.0",
    "webpack": "^5.52.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "css-loader": "^6.3.0",
    "nodemon": "^2.0.12",
    "style-loader": "^3.3.0",
    "webpack-cli": "^4.8.0"
  }
}
